{
  "name": "@slimio/tcp-sdk",
  "version": "0.5.0",
  "description": "SlimIO TCP Socket SDK",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/TCP-SDK.git"
  },
  "keywords": [
    "SlimIO",
    "monitoring",
    "tcp",
    "ip",
    "socket",
    "sdk",
    "communication",
    "callback"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/TCP-SDK/issues"
  },
  "homepage": "https://github.com/SlimIO/TCP-SDK#readme",
  "dependencies": {
    "@slimio/safe-emitter": "^1.1.0",
    "uuid": "^3.4.0",
    "zen-observable": "^0.8.15",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^4.0.0",
    "@slimio/installer": "^0.4.0",
    "@slimio/is": "^1.5.1",
    "@slimio/psp": "^0.11.0",
    "@types/zen-observable": "^0.8.0",
    "ava": "^3.3.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "fuzzbuzz": "^2.0.0",
    "husky": "^4.2.3",
    "jsdoc": "^3.6.3",
    "nyc": "^15.0.0",
    "pkg-ok": "^2.3.1",
    "premove": "^1.0.0",
    "tree-kill": "^1.2.2"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "snyk": true
}
